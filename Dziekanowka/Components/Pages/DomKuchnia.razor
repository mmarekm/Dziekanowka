    private void ZaplacZaObiad()
    {
        foreach (var skladnik in KosztaDania(wybranyObiad))
        {
            if (skladnik == "mleko") Gracz.ProduktZ(wybraneMlekoSkladnikObiad).Ilosc -= 2;
            else if (skladnik == "ser") Gracz.Dar(wybranySerSkladnikObiad).Ilosc--;
            else if (skladnik == "jajo") Gracz.ProduktZ(wybraneJajoSkladnikObiad).Ilosc -= 3;
            else if (skladnik == "tluszcz") Gracz.Dar(wybranyTluszczSkladnikObiad).Ilosc--;
            else if (skladnik == "maka") Gracz.Dar(wybranaMakaSkladnikObiad).Ilosc--;
            else if (skladnik == "bulka") Gracz.Dar(wybranaBulkaSkladnikObiad).Ilosc--;
            else if (skladnik == "mieloneMieso") Gracz.Dar(wybraneMieloneMiesoSkladnikObiad).Ilosc--;
            else if (skladnik == "swiniSzynkaLubSchab") Gracz.Dar(wybranaCzescSwiniSkladnikObiad).Ilosc--;
            else if (skladnik == "krowyLopatkaLubPoledwica") Gracz.Dar(wybranaCzescKrowySkladnikObiad).Ilosc--;
            else if (skladnik == "krowyRostbefLubAntrykotLubswiniSzynkaLubSchab") Gracz.Dar(wybranaCzescSwiniLubKrowySkladnikObiad).Ilosc--;
            else if (skladnik == "twarogLubSer") Gracz.Dar(wybranyTwarogLubSerSkladnikObiad).Ilosc--;
            else if (skladnik == "grzyb") Gracz.Grzyby.First(g => g.Nazwa == wybranyGrzybSkladnikObiad).Ilosc--;
            else if (skladnik == "kapusta") Gracz.Dar(wybranaKapustaSkladnikObiad).Ilosc--;
            else if (skladnik == "nadzienieSlodkie") Gracz.Dar(wybraneNadzienieSlodkieSkladnikObiad).Ilosc--;
            else if (skladnik == "pelnaButelka")
            {
                Gracz.Przedmiot("pelnaButelka").Ilosc--;
                Gracz.Przedmiot("pustaButelka").Ilosc++;
            }
            else if (skladnik == "kuraLubIndyk") Gracz.Dar(wybraneKuraLubIndykDoRosolu).Ilosc--;
            else if (skladnik == "makaronLubRyz") Gracz.Dar(wybranyMakaronLubRyzDoZupy).Ilosc--;
            else if (skladnik == "makaronLubZiemniak") Gracz.Dar(wybranyMakaronLubZiemniakDoZupy).Ilosc--;
            else if (skladnik == "okonLubPstrag") Gracz.Ryba(wybranyOkonLubPstragSkladnikObiad).Ilosc--;
            else if (skladnik == "borowkaLubJezynaLubPorzeczka") Gracz.Owoc(wybranaJagodaDoDzemu).Ilosc--;
            else if ((skladnik == "ziemniak" || skladnik == "ryz" || skladnik == "jeczmien")) Gracz.Dar(skladnik).Ilosc -= 3;
            else if (skladnik == "zakwasZytni") continue;
            else Gracz.Dar(skladnik).Ilosc--;
        }
    }
