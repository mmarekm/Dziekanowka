@using System.Diagnostics;
@using Dziekanowka.Mechanizm
@using Dziekanowka.Gracza
@inject LadowanieGracza ladowanieGracza
<div class="domKuchniaWidok widok rel @(widok ? "" : "przygaszony")">
    <div class="abs kuchniaDoWiatrolap"><button @onclick="async () => await NaZmianeEkranu.InvokeAsync(EkranGry.DomWiatrolap)" class="wyroznionyGuzik"><img src="img/dom/kuchnia/kuchniaDoWiatrolap.png" /></button></div>
    <div class="abs doWyciskarki"><button class="wyroznionyGuzik" @onclick="() => wyciskarka = true"><img src="img/dom/kuchnia/wyciskarka.png" /></button></div>
    <div class="abs kuchniaDoDeski"><button class="wyroznionyGuzik" @onclick="() => deska = true"><img src="img/dom/kuchnia/deska.png" /></button></div>
    <div class="abs kuchniaDoGarnka"><button class="wyroznionyGuzik" @onclick="() => garnek = true"><img src="img/dom/kuchnia/garnek.png" /></button></div>
    <div class="abs kuchniaDoPatelni"><button class="wyroznionyGuzik" @onclick="() => patelnia = true"><img src="img/dom/kuchnia/patelnia.png" /></button></div>
    <div class="abs kuchniaDoPieca"><button class="wyroznionyGuzik" @onclick="() => piec = true"><img src="img/dom/kuchnia/piec.png" /></button></div>
    <div class="abs doKawy"><button class="wyroznionyGuzik" @onclick="() => mlynek = true"><img src="img/dom/kuchnia/mlynek.png" /></button></div>
    <div class="abs doZakwasu"><button class="wyroznionyGuzik" @onclick="() => zakwas = true"><img src="img/dom/kuchnia/zakwas.png" /></button></div>
</div>
<div class="abs narzedziePrzetwornia @(wyciskarka ? "wyciskarka" : deska ? "deska" : garnek ? "garnek" : patelnia ? "patelnia" : piec ? "piec" : mlynek ? "mlynekKawa" : "zakwas")Widok @(widok ? "" : "aktywne")">
    <div class="abs kuchniaNarzedzieConfirm @(zakwas || piec || deska ? "goraPrawoIks" : "")"><button class="wyroznionyGuzik" @onclick="ZamknijNarzedzie" disabled="@(!widokPiec || !widokDeska || widoczneKosztaDania)"><img src="img/budynki/przetworniaNarzedzieConfirm.png" /></button></div>
    @if (wyciskarka)
    {
        <div class="abs handelWyciskarka"><button class="wyroznionyGuzik" @onclick="Handel"><img class="obrazekSieMiesci" src="img/dom/kuchnia/pracujWyciskarka.png" /></button></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaMarchew @(@WPortfelu("marchew") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("marchew")'><div class="abs wyciskarkalProduktIn">@WPortfelu("marchew")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaBurak @(@WPortfelu("burak") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("burak")'><div class="abs wyciskarkalProduktIn">@WPortfelu("burak")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaPomidor @(@WPortfelu("pomidor") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("pomidor")'><div class="abs wyciskarkalProduktIn">@WPortfelu("pomidor")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaAnanas @(@WPortfelu("ananas") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("ananas")'><div class="abs wyciskarkalProduktIn">@WPortfelu("ananas")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaBorowka @(@WPortfelu("borówka") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("borówka")'><div class="abs wyciskarkalProduktIn">@WPortfelu("borówka")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaBrzoskwinia @(@WPortfelu("brzoskwinia") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("brzoskwinia")'><div class="abs wyciskarkalProduktIn">@WPortfelu("brzoskwinia")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaCytryna @(@WPortfelu("cytryna") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("cytryna")'><div class="abs wyciskarkalProduktIn">@WPortfelu("cytryna")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaGrejpfrut @(@WPortfelu("grejpfrut") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("grejpfrut")'><div class="abs wyciskarkalProduktIn">@WPortfelu("grejpfrut")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaJablko @(@WPortfelu("jabłko") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("jabłko")'><div class="abs wyciskarkalProduktIn">@WPortfelu("jabłko")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaMalina @(@WPortfelu("malina") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("malina")'><div class="abs wyciskarkalProduktIn">@WPortfelu("malina")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaMorela @(@WPortfelu("morela") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("morela")'><div class="abs wyciskarkalProduktIn">@WPortfelu("morela")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaPomarancza @(@WPortfelu("pomarańcza") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("pomarańcza")'><div class="abs wyciskarkalProduktIn">@WPortfelu("pomarańcza")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaPorzeczka @(@WPortfelu("porzeczkaCzarna") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("porzeczkaCzarna")'><div class="abs wyciskarkalProduktIn">@WPortfelu("porzeczkaCzarna")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaTruskawka @(@WPortfelu("truskawka") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("truskawka")'><div class="abs wyciskarkalProduktIn">@WPortfelu("truskawka")</div></div>
        <div class="abs wyciskarkaProduktIn wyciskarkaWisnia @(@WPortfelu("wiśnia") > 0 ? "klikalneDivZielone" : "nieMoznaKliknac")" @onclick='() => NaTace("wiśnia")'><div class="abs wyciskarkalProduktIn">@WPortfelu("wiśnia")</div></div>
        <div class="abs wyciskarkaLCelow wyciskarkaLMarchwiowego">@Gracz.ProduktP("marchewSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLBuraczanego">@Gracz.ProduktP("burakSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLPomidorowego">@Gracz.ProduktP("pomidorSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLAnanasowego">@Gracz.ProduktP("ananasSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLBorowkowego">@Gracz.ProduktP("borówkaSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLBrzoskwiniowego">@Gracz.ProduktP("brzoskwiniaSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLCytrynowego">@Gracz.ProduktP("cytrynaSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLGrejpfrutowego">@Gracz.ProduktP("grejpfrutSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLJablkowego">@Gracz.ProduktP("jabłkoSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLMalinowego">@Gracz.ProduktP("malinaSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLMorelowego">@Gracz.ProduktP("morelaSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLPomaranczowego">@Gracz.ProduktP("pomarańczaSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLPorzeczkowego">@Gracz.ProduktP("porzeczkaCzarnaSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLTruskawkowego">@Gracz.ProduktP("truskawkaSok").Ilosc</div>
        <div class="abs wyciskarkaLCelow wyciskarkaLWisniowego">@Gracz.ProduktP("wiśniaSok").Ilosc</div>
        <div class="abs tacaPodajnikWyciskarka">
            @foreach (var produkt in taca)
            {
                if (produkt.Ilosc < 1) continue;
                <div class="rel produktPodajnikWyciskarka klikalneDivSzare" @onclick="() => ZTacy(produkt.Nazwa)">
                    <img class="obrazekSieMiesci" src="img/dom/kuchnia/@(produkt.Nazwa).png" />
                    <div class="abs produktIloscPodajnikWyciskarka">@produkt.Ilosc</div>
                </div>
            }
        </div>
        <div class="abs tacaEfektWyciskarka">
            @foreach (var produkt in taca)
            {
                @if (UzyskanySok(produkt.Nazwa) > 0)
                {
                    <div class="rel produktPodajnikWyciskarka">
                        <img class="obrazekSieMiesci" src="img/dom/kuchnia/@(produkt.Nazwa)Sok.png" />
                        <div class="abs produktIloscPodajnikWyciskarka czarny">@UzyskanySok(produkt.Nazwa)</div>
                    </div>
                }
            }
        </div>
    }
    @if (deska)
    {
        <div class="abs deskaDoKanapek"><button class="wyroznionyGuzik" @onclick="() => deskaKanapki = true" disabled="@(!widokDeska)"><img src="img/dom/kuchnia/deskaKanapki.png" /></button></div>
        <div class="abs deskaDoSalatek"><button class="wyroznionyGuzik" @onclick="() => deskaSalatki = true" disabled="@(!widokDeska)"><img src="img/dom/kuchnia/deskaSalatki.png" /></button></div>
        <div class="abs deskaDoSurowek"><button class="wyroznionyGuzik" @onclick="() => deskaSurowki = true" disabled="@(!widokDeska)"><img src="img/dom/kuchnia/deskaSurowki.png" /></button></div>
    }
    @if (patelnia)
    {
        <div class="abs lDrewnaPatelnia">@Gracz.Przedmiot("drewno").Ilosc</div>
        <div class="abs patelniaDania">
            @foreach (var danie in Gracz.Obiady.Where(o => Obiad.DaniaZPatelni.Any(d => d.Nazwa == o.Nazwa)))
            {
                <div class="rel patelniaDanie" @onclick='() => { widoczneKosztaDania = true; wybranyObiad = danie.Nazwa; }'>
                    <img class="obrazekSieMiesci" src="img/dom/kuchnia/patelnia/@(danie.Nazwa).png" />
                    <div class="abs lDaniaPatelnia">@danie.Ilosc</div>
                </div>
            }
        </div>
    }
    @if (garnek)
    {
        <div class="abs lDrewnaGarnek">@Gracz.Przedmiot("drewno").Ilosc</div>
        <div class="abs lWodyGarnek">@Gracz.Przedmiot("pelnaButelka").Ilosc</div>
        <div class="abs garnekDania">
            @foreach (var danie in Gracz.Obiady.Where(o => Obiad.DaniaZGarnka.Any(d => d.Nazwa == o.Nazwa)))
            {
                <div class="rel patelniaDanie" @onclick='() => { widoczneKosztaDania = true; wybranyObiad = danie.Nazwa; }'>
                    <img class="obrazekSieMiesci" src="img/dom/kuchnia/garnek/@(danie.Nazwa).png" />
                    <div class="abs lDaniaPatelnia">@danie.Ilosc</div>
                </div>
            }
        </div>
    }
    @if (widoczneKosztaDania)
    {
        <div class="abs kosztaDania">
            @{ string naczynie = patelnia ? "patelnia" : "garnek"; }
            <div class="abs rysDania"><img class="obrazekSieMiesci" src="img/dom/kuchnia/@(naczynie)/@(wybranyObiad).png" /></div>
            <div class="abs rezygnacjaZDania"><button class="wyroznionyGuzik" @onclick='WyczyscWyborDania'><img class="obrazekSieMiesci" src="img/dom/rezygnacja.png" /></button></div>
            <div class="abs skladnikiDaniaObiad">
                @foreach (var skladnik in KosztaDania(wybranyObiad))
                {
                    if (skladnik == "mleko")
                    {
                        <div class="rel skladnikDaniaObiad skladnikSzczegolowyDaniaObiad">
                            <div class="abs info3JajaPotrzebne">2</div>
                            @foreach (var zwierz in sery)
                            {
                                var obiekt = Gracz.ProduktZ($"{zwierz}Mleko");
                                <div class="rel szczegolObiad @(Gracz.JestProduktZ($"{zwierz}Mleko") ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybraneMlekoSkladnikObiad == $"{zwierz}Mleko" ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzMlekoSkladnikObiad(zwierz)'>
                                    <img class="obrazekSieMiesci" src="img/dom/kuchnia/@(obiekt.Nazwa).png" />
                                    <div class="abs lProduktuInMlynekKawa">@obiekt.Ilosc</div>
                                </div>
                            }
                        </div>
                    }
                    else if (skladnik == "ser")
                    {
                        <div class="rel skladnikDaniaObiad skladnikSzczegolowyDaniaObiad">
                            @foreach (var zwierz in sery)
                            {
                                var obiekt = Gracz.ProduktP($"{zwierz}Ser");
                                <div class="rel szczegolObiad @(Gracz.JestProduktP($"{zwierz}Ser") ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybranySerSkladnikObiad == $"{zwierz}Ser" ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzSerSkladnikObiad(zwierz)'>
                                    <img class="obrazekSieMiesci" src="img/dom/kuchnia/@(obiekt.Nazwa).png" />
                                    <div class="abs lProduktuInMlynekKawa">@obiekt.Ilosc</div>
                                </div>
                            }
                        </div>
                    }
                    else if (skladnik == "jajo")
                    {
                        <div class="rel skladnikDaniaObiad skladnikSzczegolowyDaniaObiad">
                            <div class="abs info3JajaPotrzebne">3</div>
                            @foreach (var ptak in jajka)
                            {
                                var obiekt = Gracz.ProduktZ($"{ptak}Jajo");
                                <div class="rel szczegolObiad @(Gracz.JestProduktZ($"{ptak}Jajo") ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybraneJajoSkladnikObiad == $"{ptak}Jajo" ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzJajoSkladnikObiad(ptak)'>
                                    <img class="obrazekSieMiesci" src="img/dom/kuchnia/@(obiekt.Nazwa).png" />
                                    <div class="abs lProduktuInMlynekKawa">@obiekt.Ilosc</div>
                                </div>
                            }
                        </div>
                    }
                    else if (skladnik == "tluszcz")
                    {
                        <div class="skladnikDaniaObiad skladnikSzczegolowyDaniaObiad">
                            <div class="rel szczegolObiad @(Gracz.JestProduktP("maslo") ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybranyTluszczSkladnikObiad == "maslo" ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzTluszczSkladnikObiad("maslo")'>
                                <img class="obrazekSieMiesci" src="img/dom/kuchnia/maslo.png" />
                                <div class="abs lProduktuInMlynekKawa">@Gracz.ProduktP("maslo").Ilosc</div>
                            </div>
                            <div class="rel szczegolObiad @(Gracz.JestZywnoscP("olej") ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybranyTluszczSkladnikObiad == "olej" ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzTluszczSkladnikObiad("olej")'>
                                <img class="obrazekSieMiesci" src="img/dom/kuchnia/olej.png" />
                                <div class="abs lProduktuInMlynekKawa">@Gracz.ZywnoscP("olej").Ilosc</div>
                            </div>
                        </div>
                    }
                    else if (skladnik == "bulka")
                    {
                        <div class="rel skladnikDaniaObiad skladnikSzczegolowyWiekszyDaniaObiad skladnikSzczegolowyDaniaObiad">
                            @foreach (var zboze in zboza)
                            {
                                var obiekt = Gracz.ProduktP($"{zboze}Bulka");
                                <div class="rel szczegolObiad @(Gracz.JestProduktP($"{zboze}Bulka") ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybranaBulkaSkladnikObiad == $"{zboze}Bulka" ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzBulkeSkladnikObiad(zboze)'>
                                    <img class="obrazekContain" src="img/dom/kuchnia/@(obiekt.Nazwa).png" />
                                    <div class="abs lProduktuInMlynekKawa">@obiekt.Ilosc</div>
                                </div>
                            }
                        </div>
                    }
                    else if (skladnik == "maka")
                    {
                        <div class="rel skladnikDaniaObiad skladnikSzczegolowyWiekszyDaniaObiad skladnikSzczegolowyDaniaObiad">
                            @foreach (var zboze in zboza)
                            {
                                var obiekt = Gracz.ProduktP($"{zboze}Maka");
                                <div class="rel szczegolObiad @(Gracz.JestProduktP($"{zboze}Maka") ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybranaMakaSkladnikObiad == $"{zboze}Maka" ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzMakeSkladnikObiad(zboze)'>
                                    <img class="obrazekContain" src="img/dom/kuchnia/@(obiekt.Nazwa).png" />
                                    <div class="abs lProduktuInMlynekKawa">@obiekt.Ilosc</div>
                                </div>
                            }
                        </div>
                    }
                    else if (skladnik == "swiniSzynkaLubSchab")
                    {
                        <div class="skladnikDaniaObiad skladnikSzczegolowyDaniaObiad">
                            <div class="rel szczegolObiad @(Gracz.JestProduktZ("swiniaSzynka") ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybranaCzescSwiniSkladnikObiad == "swiniaSzynka" ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzCzescSwiniSkladnikObiad("swiniaSzynka")'>
                                <img class="obrazekContain" src="img/dom/kuchnia/swiniaSzynka.png" />
                                <div class="abs lProduktuInMlynekKawa">@Gracz.ProduktZ("swiniaSzynka").Ilosc</div>
                            </div>
                            <div class="rel szczegolObiad @(Gracz.JestProduktZ("swiniaSchab") ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybranaCzescSwiniSkladnikObiad == "swiniaSchab" ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzCzescSwiniSkladnikObiad("swiniaSchab")'>
                                <img class="obrazekContain" src="img/dom/kuchnia/swiniaSchab.png" />
                                <div class="abs lProduktuInMlynekKawa">@Gracz.ProduktZ("swiniaSchab").Ilosc</div>
                            </div>
                        </div>
                    }
                    else if (skladnik == "krowyLopatkaLubPoledwica")
                    {
                        <div class="skladnikDaniaObiad skladnikSzczegolowyDaniaObiad">
                            <div class="rel szczegolObiad @(Gracz.JestProduktZ("krowaLopatka") ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybranaCzescKrowySkladnikObiad == "krowaLopatka" ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzCzescKrowySkladnikObiad("krowaLopatka")'>
                                <img class="obrazekContain" src="img/dom/kuchnia/krowaLopatka.png" />
                                <div class="abs lProduktuInMlynekKawa">@Gracz.ProduktZ("krowaLopatka").Ilosc</div>
                            </div>
                            <div class="rel szczegolObiad @(Gracz.JestProduktZ("krowaPoledwica") ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybranaCzescKrowySkladnikObiad == "krowaPoledwica" ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzCzescKrowySkladnikObiad("krowaPoledwica")'>
                                <img class="obrazekContain" src="img/dom/kuchnia/krowaPoledwica.png" />
                                <div class="abs lProduktuInMlynekKawa">@Gracz.ProduktZ("krowaPoledwica").Ilosc</div>
                            </div>
                        </div>
                    }
                    else if (skladnik == "krowyRostbefLubAntrykotLubswiniSzynkaLubSchab")
                    {
                        <div class="rel skladnikDaniaObiad skladnikSzczegolowyDaniaObiad">
                            @foreach (var mieso in miesaNaSteki)
                            {
                                <div class="rel szczegolObiad @(Gracz.JestProduktZ(mieso) ? "klikalneDivZieloneRel" : "nieMoznaKliknac") @(wybranaCzescSwiniLubKrowySkladnikObiad == mieso ? "wybranySkladnikCiasta" : "")" @onclick='() => WybierzCzescSwiniLubKrowySkladnikObiad(mieso)'>
                                    <img class="obrazekContain" src="img/przedmioty/przetwornia/@(mieso).png" />
                                    <div class="abs lProduktuInMlynekKawa">@Gracz.ProduktZ(mieso).Ilosc</div>
                                </div>
                            }
                        </div>
                    }
                    else if (skladnik == "mieloneMieso")
                    {
                        <div class="rel skladnikDaniaObiad skladnikSzczegolowyDaniaObiad">
                            @foreach (var mieso in miesaMielone)
                            {
