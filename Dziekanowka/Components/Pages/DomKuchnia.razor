        wybranaWedlinaKanapka = "";
        wybraneDodatkiKanapka.Clear();
        await ladowanieGracza.ZapiszAktualnegoGracza();
    }
    private async Task SalatkaDoPlecaka()
    {
        foreach (var owoc in wybraneOwoceSalatka) Gracz.Owoc(owoc).Ilosc--;
        if (miodNaSalatce) Gracz.ProduktZ("miod").Ilosc--;
        Gracz.Salatki.Add(new SalatkaGracza(miodNaSalatce, new HashSet<string>(wybraneOwoceSalatka)));
        wybraneOwoceSalatka.Clear();
        miodNaSalatce = false;
        await ladowanieGracza.ZapiszAktualnegoGracza();
    }
    private async Task SurowkaDoPlecaka()
    {
        foreach (var warzywo in wybraneWarzywaSurowka) Gracz.Dar(warzywo).Ilosc--;
        Gracz.ZywnoscP("sol").Ilosc--;
        if (majonezNaSurowce) Gracz.Dar("majonez").Ilosc--;
        if (!string.IsNullOrEmpty(wybraneJajoNaSurowce)) Gracz.ProduktZ(wybraneJajoNaSurowce).Ilosc--;
        Gracz.Surowki.Add(new SurowkaGracza(majonezNaSurowce, wybraneJajoNaSurowce, new HashSet<string>(wybraneWarzywaSurowka)));
        wybraneWarzywaSurowka.Clear();
        wybraneJajoNaSurowce = "";
        majonezNaSurowce = false;
        await ladowanieGracza.ZapiszAktualnegoGracza();
    }
}